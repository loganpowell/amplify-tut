{
  "name": "amplify-tut",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.2.67",
  "description": "amplify tutorial",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/loganpowell/amplify-tut",
  "homepage": "https://github.com/loganpowell/amplify-tut",
  "scripts": {
    "typewatch": "tsc --project tsconfig.json",
    "types": "tsc --project tsconfig.build.json",
    "madge": "madge --circular lib/",
    "pages": "gh-pages -d dist",
    "docs": "npm run types && npm run jsdoc",
    "tests": "npm run types && npm run madge && jest",
    "start": "webpack && webpack-dev-server --mode development",
    "build": "webpack"
  },
  "keywords": [
    "typescript",
    "starter"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-amplify/api": "latest",
    "@aws-amplify/pubsub": "latest",
    "dotenv": "^8.2.0"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "copy-webpack-plugin": "^5.1.1",
    "webpack-dev-server": "^3.10.3"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "tabWidth": 2,
    "semi": false,
    "arrowParens": "avoid",
    "bracketSpacing": true
  }
}
